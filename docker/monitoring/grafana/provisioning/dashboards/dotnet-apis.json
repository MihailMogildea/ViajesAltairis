{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "panels": [
    {
      "title": "API Health",
      "type": "stat",
      "gridPos": { "h": 4, "w": 24, "x": 0, "y": 0 },
      "targets": [{ "expr": "up{job=\"dotnet-apis\"}", "legendFormat": "{{instance}}", "refId": "A" }],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "options": { "0": { "text": "DOWN", "color": "red" } }, "type": "value" },
            { "options": { "1": { "text": "UP", "color": "green" } }, "type": "value" }
          ],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "red", "value": null }, { "color": "green", "value": 1 }] }
        }
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "background", "graphMode": "none", "orientation": "horizontal", "textMode": "auto" }
    },
    {
      "title": "HTTP Request Rate (req/s)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "targets": [
        {
          "expr": "sum(rate(http_request_duration_seconds_count{job=\"dotnet-apis\"}[5m])) by (instance)",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "reqps", "custom": { "drawStyle": "line", "lineWidth": 1, "fillOpacity": 10 } } }
    },
    {
      "title": "HTTP Request Duration (p50 / p95 / p99)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_seconds_bucket{job=\"dotnet-apis\"}[5m])) by (le, instance))",
          "legendFormat": "p50 {{instance}}",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{job=\"dotnet-apis\"}[5m])) by (le, instance))",
          "legendFormat": "p95 {{instance}}",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket{job=\"dotnet-apis\"}[5m])) by (le, instance))",
          "legendFormat": "p99 {{instance}}",
          "refId": "C"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s", "custom": { "drawStyle": "line", "lineWidth": 1, "fillOpacity": 10 } } }
    },
    {
      "title": "HTTP Error Rate (5xx)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
      "targets": [
        {
          "expr": "sum(rate(http_request_duration_seconds_count{job=\"dotnet-apis\", code=~\"5..\"}[5m])) by (instance)",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "reqps", "custom": { "drawStyle": "line", "lineWidth": 1, "fillOpacity": 10 }, "color": { "mode": "fixed", "fixedColor": "red" } } }
    },
    {
      "title": "Requests In Progress",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
      "targets": [
        {
          "expr": "http_requests_in_progress{job=\"dotnet-apis\"}",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "custom": { "drawStyle": "line", "lineWidth": 1, "fillOpacity": 10 } } }
    },
    {
      "title": "HTTP Requests by Status Code",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 20 },
      "targets": [
        {
          "expr": "sum(rate(http_request_duration_seconds_count{job=\"dotnet-apis\"}[5m])) by (code)",
          "legendFormat": "{{code}}",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "reqps", "custom": { "drawStyle": "bars", "lineWidth": 1, "fillOpacity": 50, "stacking": { "mode": "normal" } } } }
    },
    {
      "title": ".NET GC Collection Count",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 28 },
      "targets": [
        {
          "expr": "rate(dotnet_collection_count_total{job=\"dotnet-apis\"}[5m])",
          "legendFormat": "Gen{{generation}} {{instance}}",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "ops", "custom": { "drawStyle": "line", "lineWidth": 1, "fillOpacity": 10 } } }
    },
    {
      "title": ".NET Heap Size",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 28 },
      "targets": [
        {
          "expr": "system_runtime_dotnet_gc_last_collection_heap_size{job=\"dotnet-apis\"}",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "bytes", "custom": { "drawStyle": "line", "lineWidth": 1, "fillOpacity": 10 } } }
    },
    {
      "title": ".NET Thread Pool Threads",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 36 },
      "targets": [
        {
          "expr": "system_runtime_dotnet_thread_pool_thread_count{job=\"dotnet-apis\"}",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "custom": { "drawStyle": "line", "lineWidth": 1, "fillOpacity": 10 } } }
    },
    {
      "title": ".NET Process CPU Usage",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 36 },
      "targets": [
        {
          "expr": "rate(process_cpu_seconds_total{job=\"dotnet-apis\"}[5m]) * 100",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "percent", "custom": { "drawStyle": "line", "lineWidth": 1, "fillOpacity": 10 } } }
    }
  ],
  "schemaVersion": 39,
  "tags": ["dotnet", "api"],
  "templating": { "list": [] },
  "time": { "from": "now-1h", "to": "now" },
  "title": ".NET APIs",
  "uid": "altairis-dotnet"
}
